<?xml version='1.0' encoding='utf-8'?>
<widget id="com.nomateclabs.nomatecQrGenerator" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Nomatec QR Generator</name>
    <description>
        Generate QR codes from any text data.
    </description>
    <author email="support@nomateclabs.com" href="https://www.nomateclabs.com">
        Nomatec Labs
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>

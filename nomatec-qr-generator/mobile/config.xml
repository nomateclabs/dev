<?xml version='1.0' encoding='utf-8'?>
<widget id="com.nomateclabs.nomatecQrGenerator" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Nomatec QR Generator</name>
    <description>
        Generate QR codes from any text data.
    </description>
    <author email="support@nomateclabs.com" href="https://www.nomateclabs.com">
        Nomatec Labs
    </author>

    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="data:*" />
    <icon src="lg_512.png" />
    <platform name="android">
        <preference name="SplashScreenDelay" value="1000" />
        <allow-intent href="market:*" />



    </platform>
    <engine name="android" spec="^7.0.0" />
</widget>
